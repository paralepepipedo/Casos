{
    "version": 2,
    "builds": [
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "api/cases.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/cases.js"
        }
    ]
}